{
  "options": {
    "showOptionsButton": true
  },
  "user": {
    "name": "jebat"
  },
  "title": {
    "defaultTitle": "mtab",
    "dynamic": {
      "enabled": true
    },
    "faviconType": "default"
  },
  "message": {
    "enabled": true,
    "font": {
      "type": "default",
      "custom": ""
    },
    "textColor": "#f43f5e",
    "textSize": 3.75,
    "type": "afternoon-morning",
    "customText": "your custom text",
    "weather": {
      "unitsType": "f"
    }
  },
  "wallpaper": {
    "type": "fileUpload",
    "enabled": true,
    "url": "./wallpapers/bg-1.png",
    "resize": {
      "w": 1920,
      "h": 1080
    }
  },
  "ui": {
    "style": "glass",
    "glassColor": "#ffffff20",
    "blurStrength": "6px",
    "foregroundColor": "#262626",
    "backgroundColor": "#171717",
    "highlightColor": "#ffffff20",
    "cornerStyle": "round",
    "customCSS": ""
  },
  "animations": {
    "enabled": true,
    "bookmarkTiming": "left",
    "initialType": "animate-up-bouncy",
    "searchType": "animate-page-down",
    "bookmarkType": "animate-page-up"
  },
  "search": {
    "enabled": true,
    "font": {
      "type": "default",
      "custom": ""
    },
    "textColor": "#ffffff",
    "placeholderText": "search...",
    "bookmarkPlaceholderText": "find bookmark...",
    "placeholderTextColor": "#a1a1aa",
    "searchIconColor": "#14b8a6",
    "bookmarkIconColor": "#3b82f6",
    "selectIconColor": "#f59e0b",
    "engine": "duckduckgo",
    "useCustomEngine": false,
    "customEngineURL": "",
    "focusedBorderColor": "#0ea5e9",
    "assist": {
      "date": true,
      "math": true,
      "definitions": true,
      "conversions": true
    }
  },
  "hotkeys": {
    "enabled": true,
    "activationKey": " ",
    "closePageKey": "x",
    "searchBookmarksKey": "b"
  },
  "bookmarks": {
    "type": "user-defined",
    "numberKeys": false,
    "userDefinedCols": 4,
    "lineOrientation": "top",
    "userDefined": [
      {
        "type": "bookmark",
        "name": "github",
        "url": "https://github.com",
        "color": "#6366f1",
        "iconType": "ri-github-fill",
        "iconColor": "#ffffff",
        "fill": ""
      },
      {
        "type": "bookmark",
        "name": "youtube",
        "url": "https://youtube.com",
        "color": "#f43f5e",
        "iconType": "ri-youtube-fill",
        "iconColor": "#ffffff",
        "fill": ""
      },
      {
        "type": "bookmark",
        "name": "reddit",
        "url": "https://www.reddit.com",
        "color": "#f97316",
        "iconType": "ri-reddit-fill",
        "iconColor": "#ffffff",
        "fill": ""
      },
      {
        "type": "bookmark",
        "name": "chatgpt",
        "url": "https://chatgpt.com",
        "color": "#14b8a6",
        "iconType": "ri-openai-fill",
        "iconColor": "#ffffff",
        "fill": ""
      }
    ],
    "defaultBlockyCols": 4,
    "defaultBlockyColorType": "custom",
    "defaultBlockyColor": "#ffffff",
    "defaultFaviconSource": "duckduckgo",
    "showBookmarkNames": false,
    "bookmarksLocationFirefox": "toolbar"
  },
  "extras": {
    "snow": {
      "enabled": "off"
    }
  }
}
